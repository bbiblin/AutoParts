{
  "name": "proyecto-integracion",
  "private": true,
  "version": "0.0.0",
  "type": "commonjs",
  "scripts": {
    "db:seed:all": "node -r dotenv/config ./node_modules/.bin/sequelize-cli db:seed:all",
    "db:migrate": "node -r dotenv/config ./node_modules/.bin/sequelize-cli db:migrate",
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "test:ui": "vitest --ui",
    "db:create:test": "NODE_ENV=test npx sequelize-cli db:create",
    "db:migrate:test": "NODE_ENV=test npx sequelize-cli db:migrate",
    "db:seed:test": "NODE_ENV=test npx sequelize-cli db:seed:all",
    "db:reset:test": "NODE_ENV=test npx sequelize-cli db:drop && npm run db:create:test && npm run db:migrate:test",
    "test:clean": "npm run db:reset:test && npm run db:seed:test && npm run test",
    "test": "NODE_ENV=test vitest"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^6.7.2",
    "@koa/cors": "^5.0.0",
    "@koa/router": "^13.1.0",
    "@tailwindcss/vite": "^4.1.7",
    "@vitejs/plugin-react": "^4.5.0",
    "axios": "^1.9.0",
    "bootstrap": "^5.3.6",
    "dotenv": "^16.5.0",
    "framer-motion": "^12.12.2",
    "jsonwebtoken": "^9.0.2",
    "jwt-decode": "^4.0.0",
    "koa": "^3.0.0",
    "koa-body": "^6.0.1",
    "koa-bodyparser": "^4.4.1",
    "koa-logger": "^3.2.1",
    "koa-router": "^13.0.1",
    "lucide-react": "^0.510.0",
    "pg": "^8.16.0",
    "pg-hstore": "^2.3.4",
    "react": "^19.1.0",
    "react-bootstrap": "^2.10.10",
    "react-dom": "^19.1.0",
    "react-router-dom": "^7.6.0",
    "sequelize": "^6.37.7",
    "sequelize-cli": "^6.6.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.25.0",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "@vitejs/plugin-react-swc": "^3.9.0",
    "@vitest/ui": "3.2.4",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.25.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^16.0.0",
    "jsdom": "^26.1.0",
    "postcss": "^8.5.3",
    "supertest": "^7.1.1",
    "tailwindcss": "^3.4.0",
    "vite": "^6.3.5",
    "vitest": "^3.2.4"
  }
}